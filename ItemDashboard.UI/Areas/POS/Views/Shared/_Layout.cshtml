<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="Items Dashboard">
	<meta name="author" content="WajahatDeveloper">
	<meta name="keywords" content="adminkit, bootstrap, bootstrap 5, admin, dashboard, template, responsive, css, sass, html, theme, front-end, ui kit, web">

	<link rel="preconnect" href="https://www.fonts.gstatic.com/index.html">
	<link rel="shortcut icon" href="img/icons/icon-48x48.png" />

	<title>@ViewBag.Title</title>

	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter:wght@300;400;600&display=swap">

	<!-- Choose your prefered color scheme -->
	<!-- <link href="css/light.css" rel="stylesheet"> -->
	<link rel="stylesheet" href="css/dark.css">

	<!-- Grid.js CSS -->
	<link href="https://cdn.jsdelivr.net/npm/gridjs/dist/theme/mermaid.min.css" rel="stylesheet" />

	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-120946860-10"></script>
	<script>
		window.dataLayer = window.dataLayer || [];
		function gtag() { dataLayer.push(arguments); }
		gtag('js', new Date());

		gtag('config', 'UA-120946860-10', { 'anonymize_ip': true });
	</script>

</head>

<!--
HOW TO USE:
data-theme: default (default), dark, light, colored
data-layout: fluid (default), boxed
data-sidebar-position: left (default), right
data-sidebar-layout: default (default), compact
-->

<body data-theme="default" data-layout="fluid" data-sidebar-position="left" data-sidebar-layout="default">
	<div class="wrapper">

		@{
			await Html.RenderPartialAsync("_SideBar");
		}

		<div class="main">
			<main class="content">

				@{
					await Html.RenderPartialAsync("_NavBar");
				}

				@RenderBody();

			</main>

			@{
				await Html.RenderPartialAsync("_Footer");
			}

		</div>
	</div>

	<script src="js/app.js"></script>

</body>
</html>